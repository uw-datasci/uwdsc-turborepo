{
  "name": "uwdsc",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=web...",
    "dev:cxc": "turbo dev --filter=cxc...",
    "dev:docs": "turbo dev --filter=docs...",
    "build:docs": "turbo build --filter=docs",
    "lint": "turbo lint",
    "check-types": "turbo check-types",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "ui:add": "node scripts/ui-add.js"
  },
  "devDependencies": {
    "@uwdsc/eslint-config": "workspace:*",
    "@uwdsc/typescript-config": "workspace:*",
    "prettier": "^3.6.2",
    "turbo": "^2.6.1",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.22.0",
  "engines": {
    "node": ">=20"
  }
}
